    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-reconciler": "^0.26.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/openseadragon": "^2.4.5",
    "@types/react-reconciler": "^0.26.3",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5"
  },
  "peerDependencies": {
    "react": ">=16.0",
    "react-dom": ">=16.0"
