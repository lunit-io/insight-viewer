{
  "name": "@lunit/insight-viewer-docs",
  "version": "5.1.0-alpha.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf .next *.tsbuildinfo",
    "build": "yarn run clean && ENV=production next build",
    "export": "rimraf ../../dist && ENV=production next export -o ../../dist",
    "start": "next start",
    "doc": "yarn run build && yarn run export",
    "start:server": "yarn build && yarn start",
    "percy:cypress": "percy exec -- cypress run",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "test": "start-server-and-test start:server 3000 percy:cypress",
    "cypress": "start-server-and-test start:server 3000 cypress-open",
    "lint": "next lint"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.5.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@lunit/insight-viewer": "^5.2.0-alpha.0",
    "consola": "^2.15.3",
    "framer-motion": "^4",
    "msw": "^0.35.0",
    "next": "^11.1.0",
    "prismjs": "^1.24.0",
    "re-resizable": "^6.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.58",
    "@percy/cypress": "^3.1.0",
    "@types/node": "^14.14.37",
    "@types/prismjs": "^1.16.5",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "cypress": "^7.6.0",
    "eslint-config-next": "^11.1.0",
    "start-server-and-test": "^1.12.6",
    "typescript": "^4.2.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
